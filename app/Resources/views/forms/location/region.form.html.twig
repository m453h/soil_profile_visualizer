{{ form_start(form) }}
<a class="btn"  href="{{ path('region_list') }}">Back</a>
<div class="space-10"></div>
<div id="imageUploadArea" class="container">

    <div class="imageBox" id="imageUploadBox"
         data-image-upload-path="{{ path('upload_region_image') }}"
         data-image-file="{{ asset(uploadedPhoto) }}"
         data-primary-identifier="{{ app.request.get('regionCode') }}">
        <div class="thumbBox"></div>
        <div class="spinner" style="display: none"></div>
    </div>
    <div class="imageBoxAction">
        <input type="file" id="file" style="float:left;">
        <p id="btnCrop" class="btn btn-blue" value="Upload"><span class="icon-spacer-right"><i class="fa fa-upload" aria-hidden="true"></i></span>Upload</p>
        <p id="btnZoomIn" class="btn btn-green"><i class="fa fa-plus" aria-hidden="true"></i></p>
        <p id="btnZoomOut" class="btn btn-red"><i class="fa fa-minus" aria-hidden="true"></i></p>
    </div>
</div>
<div class="space-20"></div>


<div class="space-20"></div>
{{ form_end(form) }}